datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Goals {
  id                     String   @id @default(uuid()) @db.Uuid
  title                  String
  desiredWeeklyFrequency String   @map("desired_weekly_frequency")
  createdAt              DateTime @default(now()) @map("created_at")
}
